clf19020@clf19020-CFSZ6-1L:~/academic/program2/8th/meikai15-5$ cat ComputerPlayer.java
// コンピュータプレーヤを表すサブクラスComputerPlayerを書く
import java.util.Random;
public class ComputerPlayer extends JankenPlayer{
    Random rand = new Random();
    private int result;

    public ComputerPlayer(){setResult();}

    public int getResult(){return result;}

    public void setResult(){
	result = rand.nextInt(3);
    }

    public String toString(){
	System.out.println("Computer:");
	if(getResult() == 0)return "グー";
	else if(getResult() == 1)return "チョキ";
	else return "パー";
    }
}
